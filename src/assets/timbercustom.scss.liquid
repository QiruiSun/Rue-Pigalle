		&:before {
			content: "";
			position: absolute;
			width: 70%;
			height: 1px;
			bottom: 0;
			left: 0;
			background-color: #000;
			visibility: hidden;
			
			-webkit-transform: scaleX(0);
			transform: scaleX(0);
			transform-origin: top left;
			-webkit-transition: all 0.4s ease-in-out 0s;
			transition: all 0.4s ease-in-out 0s;
		}

		&:hover:before {
	  		visibility: visible;
	  		-webkit-transform: scaleX(1);
	  		transform: scaleX(1);
		}
	}
