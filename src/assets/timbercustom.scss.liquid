$overlay-size: {{ settings.shop-the-look-iconSize }};
$mobile-overlay-size: $overlay-size * 0.625; 

.overlay {
	position: absolute;
	width: $overlay-size;
	height: $overlay-size;
	background: transparent url({{ 'shop-the-look-icon.png' | asset_url }}) no-repeat center center;
	background-size: $overlay-size;
	transition: all 0.3s ease;
	border-radius: 50%;
	opacity: 0.5;
	&:hover {
		transition: all 0.2s ease;
		opacity: 0.9;
		transform: scale(1.2);
		box-shadow: 0 0 10px 5px rgba(0, 0, 0, 0.05);
	}	
}

{% assign overlay1x = settings.shop-the-look-overlay-1x | add: 0 %}
{% assign overlay1y = settings.shop-the-look-overlay-1y | add: 0 %}
{% assign overlay2x = settings.shop-the-look-overlay-2x | add: 0 %}
{% assign overlay2y = settings.shop-the-look-overlay-2y | add: 0 %}
{% assign overlay3x = settings.shop-the-look-overlay-3x | add: 0 %}
{% assign overlay3y = settings.shop-the-look-overlay-3y | add: 0 %}

.overlay-1 {
	@extend .overlay;
	left: 600px * {{ overlay1x }};
	top: 475px * {{ overlay1y }};
}
.overlay-2 {
	@extend .overlay;
	left: 600px * {{ overlay2x }};
	top: 475px * {{ overlay2y }};
}

.overlay-3 {
	@extend .overlay;
	left: 600px * {{ overlay3x }};
	top: 475px * {{ overlay3y }};
}

@include at-query ($max, $medium) {
	.overlay {
		width: $mobile-overlay-size * 1.1;
		height: $mobile-overlay-size * 1.1;
		background-size: $mobile-overlay-size * 0.5;
		border: 2px solid rgba(240, 101, 164, 0.5);
	}

	.overlay-1, .overlay-2, .overlay-3 {
		background-size: $overlay-size * 0.9;
	}

	.overlay-1 {
		@extend .overlay;
		left: 350px * {{ overlay1x }};
		top: 350px * {{ overlay1y }};
	}
	.overlay-2 {
		@extend .overlay;
		left: 350px * {{ overlay2x }};
		top: 350px * {{ overlay2y }};
	} 

	.overlay-3 {
		@extend .overlay;
		left: 350px * {{ overlay3x }};
		top: 350px * {{ overlay3y }};
	}
}

